{
  "name": "react-konami-code",
  "version": "1.1.3",
  "description": "Trigger an easter egg by pressing a sequence of keys.",
  "main": "dist/Konami.js",
  "scripts": {
    "build": "webpack --mode production",
    "test": "jest",
    "test:coverage": "jest --coverage --no-cache && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "setupFiles": [
      "./test/jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vmarchesin/react-konami-code.git"
  },
  "keywords": [
    "react",
    "konami",
    "konami code",
    "easter egg"
  ],
  "author": "Vinicius Marchesin Araujo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vmarchesin/react-konami-code/issues"
  },
  "homepage": "https://github.com/vmarchesin/react-konami-code#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "jest": "^22.4.3",
    "path": "^0.12.7",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  }
}
